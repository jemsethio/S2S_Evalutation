         RowBox[{"ParallelMap", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"Block", "[", 
             RowBox[{
              RowBox[{"{", 
               RowBox[{"data", "=", 
                RowBox[{"Import", "[", 
                 RowBox[{"#", ",", 
                  RowBox[{"{", 
                   RowBox[{"\"\<Datasets\>\"", ",", "\"\<tp\>\""}], "}"}]}], 
                 "]"}]}], "}"}], ",", "\n", "            ", 
              RowBox[{"Map", "[", 
               RowBox[{
                RowBox[{
                 RowBox[{"data", "[", 
                  RowBox[{"[", 
                   RowBox[{";;", ",", 
                    RowBox[{"#", "[", 
                    RowBox[{"[", "1", "]"}], "]"}], ",", 
                    RowBox[{"#", "[", 
                    RowBox[{"[", "2", "]"}], "]"}]}], "]"}], "]"}], "&"}], 
                ",", "points"}], "]"}]}], "]"}], "&"}], ",", 
           RowBox[{"FileNames", "[", "\"\<*.nc\>\"", "]"}]}], "]"}], ",", 
         "\n", "      ", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{"class", "\[Equal]", "\"\<Pertubation\>\""}], ",", "\n", 
           "      ", 
           RowBox[{"ParallelMap", "[", 
            RowBox[{
             RowBox[{
              RowBox[{"Block", "[", 

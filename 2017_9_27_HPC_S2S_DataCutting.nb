                  RowBox[{"Import", "[", 
                   RowBox[{"#", ",", 
                    RowBox[{"{", 
                    RowBox[{"\"\<Datasets\>\"", ",", "\"\<tp\>\""}], "}"}]}], 
                   "]"}]}], "}"}], ",", "\n", "            ", 
                RowBox[{"Map", "[", 
                 RowBox[{
                  RowBox[{
                   RowBox[{"data", "[", 
                    RowBox[{"[", 
                    RowBox[{";;", ",", ";;", ",", 
                    RowBox[{"#", "[", 
                    RowBox[{"[", "1", "]"}], "]"}], ",", 
                    RowBox[{"#", "[", 
                    RowBox[{"[", "2", "]"}], "]"}]}], "]"}], "]"}], "&"}], 
                  ",", "points"}], "]"}]}], "]"}], "&"}], ",", 
             RowBox[{"FileNames", "[", "\"\<*.nc\>\"", "]"}]}], "]"}]}], 
          "]"}]}], "]"}]}], ";", "\n", " ", 
      RowBox[{"Export", "[", 
       RowBox[{
        RowBox[{"dataset", "<>", "\"\<.mx\>\""}], ",", "result"}], "]"}]}]}], 
